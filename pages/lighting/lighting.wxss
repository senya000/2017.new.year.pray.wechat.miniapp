/* pages/lighting/lighting.wxss */
.lightingBody{
  width: 100%;
  padding: 0 24rpx 60rpx 24rpx;
  box-sizing: border-box;
  background: url('data:image/jpeg;base64,');
  background-size: 100% 100%;
}
.lightingType{
  font-size: 40rpx;
  text-align: center;
  padding: 20rpx 0 40rpx 0;
}
.lightingBigBox{
  display: flex;
}
.lightingLeft{
  flex: 2.5;
}
.lightingBox{
  margin-bottom: 40rpx;
}
.lightingTitle{
  font-weight: bold;
  font-size: 34rpx;
  margin-bottom: 12rpx;
}
.lightingText{
  font-size: 30rpx;
  padding: 0 36rpx
} 
.lightingRight{
  flex: 1;
  padding: 100rpx 0;
  display: flex;
  flex-direction: column;
  align-items: center;
}
.lightingRight>image{
  width: 210rpx;
  height: 290rpx;
  background-color: transparent;
}
.lightingRight>view{
  margin-top: 24rpx;
  font-size: 32rpx;
}
.lightingTip{
  color: #ab0000;
  font-size: 22rpx;
  padding: 24rpx 0;
}
.lightingFrom{
  font-size: 32rpx;
  margin-bottom: 40rpx;
}
.lightingFrom>view{
  border-bottom: 1px solid #f4f4f4; 
}
 .fromControl{ 
   padding: 26rpx 0;
} 
.fromControl>view{
  float: left;
}
 .fromTitle{
  line-height: 80rpx;  
 } 
 .fromControl>text{
   display: inline-block;
}
.fromControl>input{
  float: right;
  width: 84%;
  margin-top: -2rpx;
  font-size: 30rpx;
  color: #808080;
  height: 50rpx;
  padding: 0 10rpx;
  background-color: #fff;
}
.fromControl>textarea{
  border: 1px solid #f4f4f4; 
  padding: 14rpx;
  line-height: 38rpx;
  margin-top: 16rpx;
  font-size: 30rpx;
  color: #808080;
  background-color: #fff;
  width: 80%;
  display: inline-block;
  vertical-align: top;
}
.fromBtn{
  width: 78%;
  height: 76rpx;
  line-height: 76rpx;
  background-color: #ed7333;
  border-radius: 10rpx;
  color: #fff;
  text-align: center;
  margin: 0 auto;
  font-size: 32rpx;
  margin-bottom: 80rpx;
}
picker{
  display: inline-block;
  width: 85%;
  background-color: #fff;
  height: 50rpx;
}
.picker{
  width: 100%;
  display: inline-block;
}
.wishText{
  display: inline-block;
  margin-top: 14rpx;
  color: #000;
}